<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="337.0" prefWidth="454.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.myanimelist.controllers.profiles.ProfileUserController">
      <VBox layoutX="38.0" layoutY="33.0" prefHeight="148.0" prefWidth="149.0">
         <children>
            <TextField fx:id="emailLabel" promptText="Email" />
            <TextField fx:id="nameLabel" layoutY="15.0" promptText="Username">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="passLabel" layoutY="15.0" promptText="Password">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="confirmLabel" promptText="Confirm Password">
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
      <Button fx:id="saveBut" layoutX="162.0" layoutY="254.0" mnemonicParsing="false" onAction="#onSave" prefHeight="25.0" prefWidth="177.0" text="Save" />
   <ImageView fx:id="img" fitHeight="150.0" fitWidth="200.0" layoutX="239.0" layoutY="32.0" onMouseClicked="#changeImg" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@../icons/default.png" />
      </image></ImageView>
</AnchorPane>
